		"husky": "^4.3.8",
		"mocha": "^8.0.1",
		"mocha-junit-reporter": "^2.0.2",
		"mock-require": "^3.0.3",
		"nyc": "^15.0.0",
		"octicons": "^7.4.0",
		"prettier": "^2.0.5",
		"pretty-quick": "^3.1.0",
