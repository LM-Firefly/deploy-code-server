{
	"name": "docs-article-templates",
	"displayName": "learn-article-templates",
	"description": "Learn article templates",
	"icon": "images/docs-logo-ms.png",
	"aiKey": "f4846ccb-3a94-40b3-b34c-023d6ef9fd4d",
	"version": "1.0.8",
	"publisher": "docsmsft",
	"repository": {
		"type": "git",
		"url": "https://ceapex@dev.azure.com/ceapex/Engineering/_git/learn-article-templates"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-docs-authoring/issues"
	},
	"homepage": "https://github.com/Microsoft/vscode-docs-authoring",
	"engines": {
		"vscode": "^1.71.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown",
		"onLanguage:yaml"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "applyTemplate",
				"title": "Template",
				"category": "Learn",
				"icon": "node_modules/octicons/build/svg/file.svg"
			},
			{
				"command": "workbench.action.openSettings2",
				"title": "Open Settings (JSON)",
				"category": "Preferences",
				"description": "Opens the settings JSON file where you can manually make changes."
			}
		],
		"configuration": {
			"type": "object",
			"title": "Learn Article Templates Extension Configuration",
			"properties": {
				"docs.templates.alias": {
					"type": "string",
					"default": "",
					"description": "Microsoft alias"
				},
				"docs.templates.githubid": {
					"type": "string",
					"default": "",
					"description": "GitHub ID"
				},
				"docs.templates.template_repo": {
					"type": "string",
					"default": "https://github.com/MicrosoftDocs/content-templates/archive/main.zip",
					"description": "Learn template repo"
				},
				"docs.templates.docs_token": {
					"type": "string",
					"default": "",
					"description": "Microsoft Docs token to utilize AI features"
				},
				"docs.templates.ai_token": {
					"type": "string",
					"default": "",
					"description": "Azure Open AI token to utilize AI features"
				}
			}
		},
		"menus": {
			"commandPalette": [
				{
					"command": "applyTemplate",
					"group": "Learn"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"prettier": "prettier --write \"**/*.{ts,js,json,yml}\"",
		"prettier-check": "prettier --check \"**/*.{ts,js,json,yml}\" --loglevel debug",
		"pretest": "npm run compile",
		"test": "node ./out/src/test/runTest.js",
		"lint": "eslint . --ext .ts",
		"precommit": "pretty-quick --staged --pattern \"**/*.{ts,js,json,yml}\"",
		"pre-push": "npm run compile && npm run lint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged --pattern \"**/*.{ts,js,json,yml}\"",
			"pre-push": "npm run pre-push"
		}
	},
	"dependencies": {
		"axios": "^1.6.7",
		"extract-zip": "^2.0.1",
		"front-matter": "^4.0.2",
		"js-yaml": "^3.13.1",
		"node-dir": "^0.1.17",
		"node-downloader-helper": "^1.0.18",
		"os": "^0.1.2",
		"tmp": "^0.2.1",
		"vscode-extension-telemetry": "^0.1.1"
	},
	"devDependencies": {
